script.
    class GameOfLife {
        constructor() {
            this.w = Math.floor(windowWidth);
            this.h = Math.floor(windowWidth / 2);
            this.resolution = 5;
            this.grid;
            this.cols;
            this.rows;
        }
        setup() {
            let canvas = createCanvas(this.w, this.h);
            canvas.position(0,0,'absolute');
            this.cols = this.w / this.resolution;
            this.rows = this.h / this.resolution;
            this.grid = this.make2DArray(this.cols, this.rows);
            for(let i = 0; i < this.cols; i++){
                for(let j = 0; j < this.rows; j++){
                    this.grid[i][j] = floor(random(2));
                }
            }
        }
        make2DArray(cols, rows){
            let arr = new Array(cols);
            for(let i = 0; i < arr.length; i++){
                arr[i] = new Array(rows);
            }
            return arr;
        }
        draw() {
            background(0, 1);
            for(let i = 0; i < this.cols; i++){
                for(let j = 0; j < this.rows; j++){
                    let x = i * this.resolution;
                    let y = j * this.resolution;
                    if(this.grid[i][j] == 1)  {
                        fill(255, 100);
                        noStroke();
                        rect(x,y, this.resolution, this.resolution);
                    }
                }
            }
            let next = this.make2DArray(this.cols, this.rows);
            //compute next based on grid
            for(let i = 0; i < this.cols; i++){
                for(let j = 0; j < this.rows; j++){
                    let state = this.grid[i][j];
                    if(i == 0 || i == this.cols - 1 || j == 0 || j == this.rows - 1){
                        next[i][j] = state;
                    }
                    let sum = 0;
                    let neighbors = this.countNeighbors(this.grid, i, j);
                    if(state == 0 && neighbors == 3) {
                        next[i][j] = 1;
                    } else if(state == 1 && (neighbors < 2 || neighbors > 3)){
                        next[i][j] = 0;
                    } else {
                        next[i][j] = state;
                    }
                }
            }
            this.grid = next;
        }
        countNeighbors(grid, x, y){
            let sum = 0;
            for(let i = -1; i < 2; i++){
                for(let j = -1; j < 2; j++){
                    let col = (x + i + this.cols) % this.cols;
                    let row = (y + j + this.rows) % this.rows;
                    //console.log(col + ' ' + row + ' ' + grid[col][row])
                    sum += grid[col][row];
                }
            }
            sum -= grid[x][y];
            return sum;
        }
    }
    let gol;
    function setup() {
        gol = new GameOfLife();
        gol.setup();
    }
    function draw(){
        gol.draw();
    }
